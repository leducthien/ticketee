<%= form_for([@project, @ticket], html: {multipart: true}) do |f| %>
  <% @ticket.errors.full_messages.each do |e| %>
    <%= e %>
  <% end %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :asset, 'File' %>
  <%= f.file_field :asset %>

  <%= f.submit %>

<% end %>
